{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug payroll",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/time-series/payroll/data_visualization.py", // to debug sagemaker
      "console": "integratedTerminal",
